<% bookInst.forEach(book => { %>
        <div>
                <img src=<%= book.bookImg %>>
                <h5><%= book.title %></h5>
                <p><%= book.author %></p>
                <p><%= book.description %></p>
                <p><%= book.ISBN %></p>
                <p><%= book.bookshelf %></p>
        </div>
<% }) %>